import java.util.*;
public class Main {
    public static void main(String args[]) {
     Scanner s=new Scanner(System.in);
	 int n=s.nextInt();
	 int arr[]=new int[n];
	 for(int i=0;i<n;i++)
	 arr[i]=s.nextInt();

f(arr,n);
for(Integer elem: arr)
System.out.print(elem+" ");

    }
	public static void f(int arr[], int n){
int zcount=0;
for(int i=0;i<n;i++){
if(arr[i]==0)
zcount++;
}

for(int i=0;i<n;i++){
    if(i<zcount)
    arr[i]=0;
    else
    arr[i]=1;
}

	}
}
