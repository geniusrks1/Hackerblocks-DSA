import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++){
			arr[i]=s.nextInt();
		}
		int m=s.nextInt();
	int ans=f(arr, n,m);
	System.out.print(ans);

    }
	public static int f(int arr[], int n, int m){
		for(int i=0;i<n;i++)
		if(arr[i]==m)
		return i;

		return -1;
	}
}
