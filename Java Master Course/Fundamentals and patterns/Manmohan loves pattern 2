import java.util.*;
public class Main {
    public static void main(String args[]) {
Scanner s=new Scanner(System.in);
int n=s.nextInt();

for(int i=0;i<n;i++){
	for(int j=0;j<=i;j++){
if(i<=1){
	System.out.print('1');
}
else{
	if(j==0 || j==i)
    System.out.print(i);
    else
    System.out.print('0');

}

	}
	System.out.println();
}

    }
}
