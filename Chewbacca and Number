
//1. c++ code
#include<iostream>
using namespace std;
int main() {
	long  n;
	cin>>n;
	long  x=0;
	long  base=1;
	while(n!=0){
	int lastdigit=n%10;
	int revdigit=0;
		if(n==9)
        revdigit=9;
		else
		revdigit=lastdigit>=5?9-lastdigit:lastdigit;

		x+=base*revdigit;
		base*=10;
		n=n/10;
	}
	cout<<x<<endl;
}

